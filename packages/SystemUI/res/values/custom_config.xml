<resources>
    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">4</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">5</integer>

    <!-- Color of the FOD view -->
    <color name="config_fodColor">#00ff00</color>
    <color name="config_fodColorBackground">#20000000</color>

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <bool name="config_fingerprintWakeAndUnlock">true</bool>

    <!-- Allow devices override audio panel location to the left side -->
    <bool name="config_audioPanelOnLeftSide">false</bool>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">true</bool>

    <!-- Allow devices to hide ringer button above volume slider -->
    <bool name="config_hideRingerButton">false</bool>

    <!-- Whether to show VoLTE icon in the status bar -->
    <bool name="config_display_volte">true</bool>

    <!-- Whether to show VoWiFi icon in the status bar -->
    <bool name="config_display_vowifi">true</bool>
</resources>
